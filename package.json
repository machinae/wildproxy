    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "url-parse": "^1.5.9"
  }
}
